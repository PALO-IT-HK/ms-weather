[
    {
        "name": "%s",
        "image": "%s.dkr.ecr.%s.amazonaws.com/%s:%s",
        "essential": true,
        "portMappings": [
            {
                "containerPort": 3000,
                "hostPort": 0,
                "protocol": "tcp"
            }
        ],
        "cpu": 0,
        "memoryReservation": 256,
        "environment": [
            {
                "name": "NODE_ENV",
